<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.citi.training.groupb.servicedemo.mapper.SharesMapper">
    <update id="updateSharesNum" parameterType="com.citi.training.groupb.servicedemo.vo.SharesNumUpdate">
        UPDATE shares
        SET shares_num=#{newNum}
        WHERE shares.RIC=#{ric};
    </update>
</mapper>
